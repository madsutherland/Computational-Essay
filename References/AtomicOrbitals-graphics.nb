Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"\[CapitalOmega]", "(", "r", ")"}], 
    RowBox[{"\[DifferentialD]", "r"}]}], "=", 
   RowBox[{
    StyleBox["{",
     SpanMaxSize->Infinity], GridBox[{
      {
       RowBox[{
        RowBox[{"\[LeftBracketingBar]", 
         RowBox[{
          SubscriptBox["\[Psi]", 
           RowBox[{"n", ",", "l", ",", "m"}]], "(", "r", ")"}], 
         "\[RightBracketingBar]"}], " ", 
        RowBox[{"\[DifferentialD]", "r"}]}], Cell[
       "pure basis state",ExpressionUUID->
        "20773cd0-416b-4cca-9150-d081bec5ba6a"], 
       RowBox[{
        RowBox[{
         SubscriptBox["\[Psi]", 
          RowBox[{"n", ",", "l", ",", "m"}]], "(", "r", ")"}], ","}]},
      {
       RowBox[{
        RowBox[{"\[LeftBracketingBar]", 
         RowBox[{
          SubscriptBox["\[Psi]", 
           RowBox[{"n", ",", "l"}]], "(", "r", ")"}], 
         "\[RightBracketingBar]"}], 
        RowBox[{"\[DifferentialD]", "r"}]}], Cell[
       "coherently mixed state",ExpressionUUID->
        "8815b7fb-c7b4-4ddd-ac9b-a4f8ef8660b8"], 
       RowBox[{
        RowBox[{
         RowBox[{
          SubscriptBox["\[Psi]", 
           RowBox[{"n", ",", "l"}]], "(", "r", ")"}], "=", 
         RowBox[{
          UnderscriptBox["\[Sum]", "m"], 
          RowBox[{
           SubscriptBox["c", "m"], 
           RowBox[{
            SubscriptBox["\[Psi]", 
             RowBox[{"n", ",", "l", ",", "m"}]], "(", "r", ")"}]}]}]}], 
        ","}]}
     }], "\[AutoRightMatch]"}]}], TraditionalForm]], "DisplayFormula",Expressi\
onUUID->"24c4f508-a2ee-4317-b75d-51b1bd6b2530"],

Cell["where ", "Text",ExpressionUUID->"91d27130-fef6-4f1e-a8a6-5caab58666e4"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{
