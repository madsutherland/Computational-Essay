Cell["\<\
For the hydrogen atom the radial wavefunction in atomic units is\
\>", "Text",ExpressionUUID->"9d0669da-1c17-4319-a70a-7765678a782f"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{
     SubscriptBox["R", 
      RowBox[{"n", ",", "l"}]], "(", "r", ")"}], "=", 
    RowBox[{
     RowBox[{"-", 
      SqrtBox[
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          FractionBox[
           RowBox[{"2", "Z"}], "n"], ")"}], "3"], 
        FractionBox[
         RowBox[{
          RowBox[{"(", 
           RowBox[{"n", "-", "l", "-", "1"}], ")"}], "!"}], 
         RowBox[{"2", "n", " ", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"n", "+", "l"}], ")"}], "!"}]}]]}]]}], 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       RowBox[{"-", "\[Rho]"}], "/", "2"}]], 
     SuperscriptBox["\[Rho]", "l"], " ", 
     RowBox[{
      SubsuperscriptBox["L", 
       RowBox[{"n", "-", "l", "-", "1"}], 
       RowBox[{
        RowBox[{"2", " ", "l"}], "+", "1"}]], "(", "\[Rho]", ")"}]}]}], ","}],
   TraditionalForm]], "DisplayFormula",ExpressionUUID->"090d747b-1aec-453b-\
bb50-0d677f96be56"],

Cell["where ", "Text",ExpressionUUID->"a459643c-0a64-41f7-a8ab-418e784b1c59"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"\[Rho]", "=", 
    FractionBox[
     RowBox[{"2", " ", "Z", " ", "r"}], "n"]}], ";"}], 
  TraditionalForm]], "InputOnly",
 InitializationCell->
  True,ExpressionUUID->"7e9ba4ec-c0fa-4bae-b2ec-7de61d621bcf"],

Cell[TextData[{
 "and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["L", "n", "\[Alpha]"], "(", "x", ")"}], 
   TraditionalForm]],ExpressionUUID->"d592cb98-18d4-442f-adfe-506a378fe459"],
 " denote the associated Laguerre Polynomials as defined by ",
 ButtonBox["Abramowitz and Stegun",
  BaseStyle->"Hyperlink",
  ButtonData:>"Abramowitz and Stegun (1972)"],
 ". Although the definition of the associated Laguerre Polynomials differs \
from that used in many quantum mechanics texts, it has several advantages: it \
is the standard used by most mathematicians and several books of tables; the \
number of nodes of the radial wavefunction equals the order of the associated \
Laguerre Polynomial, i.e., ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "-", "l", "-", "1"}], TraditionalForm]],ExpressionUUID->
  "b1da7f45-2017-43c9-9fb2-cae47472ee51"],
 "; and it is the definition used in ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 ". "
}], "Text",ExpressionUUID->"faa51aab-d5c9-44a8-81f9-eb2d0c6f92fd"],

