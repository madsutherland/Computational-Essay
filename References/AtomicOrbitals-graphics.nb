}], "Text",ExpressionUUID->"b9b00921-73e0-4def-9bfb-746d6749a055"],

Cell["In general, the expression", "Text",ExpressionUUID->"f601f991-9c49-4962-9932-67c72d5ef08b"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{
     SubscriptBox["\[Omega]", "l"], "(", 
     RowBox[{"\[Theta]", ",", "\[Phi]"}], ")"}], "=", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{
       RowBox[{"m", "=", 
        RowBox[{"-", "l"}]}], ",", "2"}], "l"], 
     RowBox[{
      SubscriptBox["a", "m"], 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"\[ImaginaryI]", " ", 
        SubscriptBox["b", "m"]}]], 
      RowBox[{
       SubsuperscriptBox["Y", "l", "m"], "(", 
       RowBox[{"\[Theta]", ",", "\[Phi]"}], ")"}]}]}]}], ","}], 
  TraditionalForm]], "DisplayFormula",ExpressionUUID->"a8e64e24-8737-4169-\
994a-4aecc747173f"],

Cell[TextData[{
