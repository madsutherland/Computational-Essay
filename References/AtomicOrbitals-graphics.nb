Cell[BoxData[
 FormBox[
  RowBox[{
   FormBox[
    RowBox[{
     RowBox[{
      RowBox[{
       SubsuperscriptBox["Y", 
        SubscriptBox["l", "1"], 
        SubscriptBox["m", "1"]], "(", 
       RowBox[{"\[Theta]", ",", "\[Phi]"}], ")"}], " ", 
      RowBox[{
       SubsuperscriptBox["Y", 
        SubscriptBox["l", "2"], 
        SubscriptBox["m", "2"]], "(", 
       RowBox[{"\[Theta]", ",", "\[Phi]"}], ")"}]}], "=", 
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{
        SubscriptBox["l", "3"], "=", 
        RowBox[{"\[LeftBracketingBar]", 
         RowBox[{
          SubscriptBox["l", "1"], "-", 
          SubscriptBox["l", "2"]}], "\[RightBracketingBar]"}]}], 
       RowBox[{
        SubscriptBox["l", "1"], "+", 
        SubscriptBox["l", "2"]}]], 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"m", "=", 
         RowBox[{"-", 
          SubscriptBox["l", "3"]}]}], 
        SubscriptBox["l", "3"]], 
       RowBox[{
        SqrtBox[
         FractionBox[
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"2", 
              SubscriptBox["l", "1"]}], "+", "1"}], ")"}], 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"2", 
              SubscriptBox["l", "2"]}], "+", "1"}], ")"}], 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"2", 
              SubscriptBox["l", "3"]}], "+", "1"}], ")"}]}], 
          RowBox[{"4", "\[Pi]"}]]], 
        RowBox[{"(", GridBox[{
           {
            SubscriptBox["l", "1"], 
            SubscriptBox["l", "2"], 
            SubscriptBox["l", "3"]},
           {"0", "0", "0"}
          }], ")"}], 
        RowBox[{"(", GridBox[{
           {
            SubscriptBox["l", "1"], 
            SubscriptBox["l", "2"], 
            SubscriptBox["l", "3"]},
           {
            SubscriptBox["m", "1"], 
            SubscriptBox["m", "2"], 
            SubscriptBox["m", "3"]}
          }], ")"}], " ", 
        SuperscriptBox[
         RowBox[{
          SubsuperscriptBox["Y", 
           SubscriptBox["l", "3"], 
           SubscriptBox["m", "3"]], "(", 
          RowBox[{"\[Theta]", ",", "\[Phi]"}], ")"}], "*"], " "}]}]}]}],
    TraditionalForm], "."}], TraditionalForm]], "DisplayFormula",ExpressionUUI\
D->"fb3dad5d-742b-42be-abb1-b8359997b0ea"],

