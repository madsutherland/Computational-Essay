Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["\[ScriptCapitalN]", 
     RowBox[{"n_", ",", "l_", ",", 
      RowBox[{"Z_:", "1"}]}]], "=", 
    RowBox[{"-", 
     SqrtBox[
      RowBox[{
       SuperscriptBox[
        RowBox[{"(", 
         FractionBox[
